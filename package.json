{
  "name": "@qunnovera/numfy",
  "version": "0.1.4",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "dev": "tsc -p tsconfig.dev.json && bun tools/build.ts copy-assets --dev",
    "build": "tsc -p tsconfig.build.json && bun tools/build.ts copy-assets",
    "clean": "bun tools/clean.ts",
    "release": "bun run clean && bun run build && bun tools/publish.ts",
    "test": "bun test"
  },
  "author": "sharad <sharadtomer@outlook.com>",
  "license": "MIT",
  "homepage": "https://github.com/sharadtomer/numfy",
  "description": "Complete excel like formula engine",
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.6.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "excel",
    "calc",
    "formula",
    "formula parser",
    "numfy",
    "typescript"
  ],
  "dependencies": {
    "@sharadt/lexicons": "^0.1.6"
  }
}
